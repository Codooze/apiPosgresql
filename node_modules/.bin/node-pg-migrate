#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/b/Projects/apiPosgresql/node_modules/.pnpm/node-pg-migrate@6.2.2_pg@8.10.0/node_modules/node-pg-migrate/bin/node_modules:/mnt/b/Projects/apiPosgresql/node_modules/.pnpm/node-pg-migrate@6.2.2_pg@8.10.0/node_modules/node-pg-migrate/node_modules:/mnt/b/Projects/apiPosgresql/node_modules/.pnpm/node-pg-migrate@6.2.2_pg@8.10.0/node_modules:/mnt/b/Projects/apiPosgresql/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/b/Projects/apiPosgresql/node_modules/.pnpm/node-pg-migrate@6.2.2_pg@8.10.0/node_modules/node-pg-migrate/bin/node_modules:/mnt/b/Projects/apiPosgresql/node_modules/.pnpm/node-pg-migrate@6.2.2_pg@8.10.0/node_modules/node-pg-migrate/node_modules:/mnt/b/Projects/apiPosgresql/node_modules/.pnpm/node-pg-migrate@6.2.2_pg@8.10.0/node_modules:/mnt/b/Projects/apiPosgresql/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-pg-migrate/bin/node-pg-migrate" "$@"
else
  exec node  "$basedir/../node-pg-migrate/bin/node-pg-migrate" "$@"
fi
